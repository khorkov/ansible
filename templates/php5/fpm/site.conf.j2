[site]
listen = /var/run/php5-site.sock
listen.mode = 0666
user = site
group = site
chdir = /var/www/site
php_admin_value[upload_tmp_dir] = /var/www/site/tmp
php_admin_value[soap.wsdl_cache_dir] = /var/www/site/tmp
php_admin_value[date.timezone] = Europe/Moscow
pm = dynamic
pm.max_children = 10
pm.start_servers = 2
pm.min_spare_servers = 2
pm.max_spare_servers = 4